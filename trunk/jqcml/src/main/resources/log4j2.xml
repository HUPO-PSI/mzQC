<?xml version="1.0" encoding="utf-8"?>
<configuration status="trace">

	<appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</Console>
	</appenders>
	
	<loggers>
		<root level="trace">
			<appender-ref ref="console" />
		</root>
	</loggers>
	
</configuration>