    <runQuality ID="ID001">
        <metaDataParameters>
            <InputFiles>
                <RawFile location="ftp://rawdump.org/supergoodrunsfrom2525/myfile1.raw" id="ID005" name="myfile1.raw">
                    <FileFormat>
                        <cvParam cvRef="PSI-MS" accession="MS:1000563" name="Thermo RAW format"/>
                    </FileFormat>
                    <userParam type="xsd:string" name="experiment-omics-type" description="experiment-omics-type" value="metabolomics-mrm"/>
                </RawFile>
                <IdentificationFile searchDatabase_name="my-spectral-library" location="z:\somefolder\allmyidfiles\myidentificationsfile.isatab" id="ID009" name="myidentificationsfile.isatab">
                    <FileFormat>
                        <userParam type="xsd:string" name="metabo-id-file-type" description="metabo-id-file-type" value="isa-tab"/> <!--actually, the schema does not support userParams at this place for good reasons, we need a CV term. What is your input? -->
                    </FileFormat>
                </IdentificationFile>
                <QuantificationFile location="z:\somefolder\allmyquantfiles\myquantsfile.isatab" id="ID011" name="myquantsfile.isatab">
                    <FileFormat>
                        <userParam type="xsd:string" name="metabo-quant-file-type" description="metabo-quant-file-type" value="isa-tab"/> <!--actually, the schema does not support userParams at this place for good reasons, we need a CV term. What is your input? -->
                    </FileFormat>
                </QuantificationFile>
                <OtherFile location="z:\somefolder\calibrations\mycalibrationsfile.isatab" id="ID013" name="mycalibrationsfile.isatab">
                    <ExternalFormatDocumentation>http://www.mycalibrationform.at/</ExternalFormatDocumentation>
                    <FileFormat>
                        <userParam type="xsd:string" name="???" description="???" value="???"/> <!--actually, the schema does not support userParams at this place for good reasons, we need a CV term. What is your input? -->
                    </FileFormat>
                </OtherFile>
            </InputFiles>
<!--	<cvParam cvRef="PSI-QC-CV" accession="???" name="sample work-up procedure" description="value describe the " value="precipitation, solid phase extraction, ultrafiltration, recovery, aliquots" /> -->
        </metaDataParameters>
	<!-- Generally, what should be known after the metadata section - Is the run a  :
		# spike-in? which standard? AKA internal standard 
			* which calibration points
			* bias on calibration points, LLOQ and ULOQ of instrumentation (separate or just LOQ)?
			* slope, R^2
			* concentration within LLOQ and ULOQ
			* control or calibration?
		# external standard? (https://goldbook.iupac.org/html/E/E02290.html)
			* same as with internal
		# blank? I would _strongly_ recommned to avoid calling something containing a standard (internal or not does not matter) a blank 
		# SST?	
			* resolution
			* RSD
			* column efficiency
			* tailing factor 
		# Sample/Unknowns
		# Also what the sample work-up procedure is (as cv collection in metadata?)
	-->
	
<!-- These metrices should be present according to data available in an SST -->
	<!-- * resolution R -->
        <qualityMetric ID="ID261" cvRef="PSI-MS" accession="MS:1000011" name="mass resolution" value="60000"/>
	<!-- * column efficiency N -->
	<qualityMetric ID="ID262" cvRef="PSI-QC-CV" accession="QC:???????" name="column efficiency" value="1000"/>
	<!-- * tailing factor T -->
        <qualityMetric ID="ID263" cvRef="PSI-MS" accession="QC:???????" name="tailing factor" value="1.1"/>
	<!-- * Asymmetry A? -->
        <qualityMetric ID="ID264" cvRef="PSI-MS" accession="QC:???????" name="chromatographic peak asymmetry" value="1.05"/>
	<!-- * void time Tm? -->
        <qualityMetric ID="ID265" cvRef="PSI-MS" accession="QC:???????" name="column dead time" value="2" unitAccession="UO:0000010" unitName="second" unitCvRef="UO"/>
	<!-- can you specify in which contexts you need/calculate/get reported a s/n? -->
 	
	<!-- * autosampler wash solvent A [mL] -->
	<!-- * autosampler wash solvent B [mL] -->
	<qualityMetric ID="ID266" cvRef="PSI-QC-CV" accession="QC:???????" name="autosampler wash solvent">
	    <content ID="ID166" cvRef="PSI-QC-CV" accession="linked lists" name="linked lists" value='2'>{[A,B],'UO:0000098':[123,345]}</content>
	</qualityMetric>
		<!-- * mobile phase A [mL] -->
		<!-- * mobile phase B [mL] -->
	<qualityMetric ID="ID267" cvRef="PSI-QC-CV" accession="QC:???????" name="autosampler wash solvent">
		<content ID="ID167" cvRef="PSI-QC-CV" accession="linked lists" value='2'>{[A,B],'UO:0000098':[123,345]}</content>
	</qualityMetric>
		<!-- * Buffer solution pump A (%) -->
		<!-- * Buffer solution pump B (%) -->
	<qualityMetric ID="ID268" cvRef="PSI-QC-CV" accession="QC:???????" name=" ">
		<content ID="ID168" cvRef="PSI-QC-CV" accession="linked lists" value='2'>{[A,B],'UO:0000165':[11,3]}</content>
	</qualityMetric>
	<!-- * Autosampler waste container fill (%) -->
	<qualityMetric ID="ID269" cvRef="PSI-MS" accession="QC:???????" name="waste container fill" value="23" unitAccession="UO:0000187" unitName="percent" unitCvRef="UO"/>

	<!-- * N2 gas pressure curtain [psi] -->
	<!-- * N2 gas pressure source [psi] -->
	<!-- * N2 gas pressure exhaust [psi] -->
	<qualityMetric ID="ID270" cvRef="PSI-QC-CV" accession="QC:???????" name="N2 gas pressure">
		<!-- using SI units here! (pascal) -->
		<content ID="ID170" cvRef="PSI-QC-CV" accession="linked lists" value='3'>{[curtain,source,exhaust],'UO:0000110':[620528.4,613633.6,696370.8]}</content>
	</qualityMetric>
	<!-- * Pump equilibration back pressure [psi] -->
	<qualityMetric ID="ID271" cvRef="PSI-MS" accession="QC:???????" name="Pump equilibration back pressure" value="716370.8" unitAccession="UO:0000110" unitName="pascal" unitCvRef="UO"/>
	
	<!-- * complete injection of sample (uL) -->
	<qualityMetric ID="ID272" cvRef="PSI-MS" accession="QC:???????" name="injected sample volume" value="34" unitAccession="UO:0000101" unitName="microliter" unitCvRef="UO"/>
	<!-- * RT calibration substance pyr (light) [min] -->
	<!-- * RT calibration substance g6p (light) [min] -->
	<qualityMetric ID="ID273" cvRef="PSI-QC-CV" accession="QC:???????" name="N2 gas pressure">
		<!-- using SI units here! (pascal) -->
		<content ID="ID173" cvRef="PSI-QC-CV" accession="linked lists" value='3'>{[curtain,source,exhaust],'UO:0000110':[620528.4,613633.6,696370.8]}</content>
	</qualityMetric>
	<!-- * Ioncount (detector count?) substance pep (light) -->
	<!-- * Ioncount (detector count?) substance atp (light) -->
	<qualityMetric ID="ID274" cvRef="PSI-QC-CV" accession="QC:???????" name="Ioncount on indicator substance">
		<content ID="ID174" cvRef="PSI-QC-CV" accession="linked lists" value='2'>{[pep light, atp light],'MS:1000131':[6E+5,3.4E+8]}</content>
	</qualityMetric>
	<!-- * Peakdetection indicator substance pep (light) -->
	<!-- * Peakdetection indicator substance pep (heavy) -->
	<!-- * Peakdetection indicator substance gln (light) -->
	<!-- * Peakdetection indicator substance gln (heavy) -->
	<qualityMetric ID="ID275" cvRef="PSI-QC-CV" accession="QC:???????" name="Peak detection indicator substance">
		<!-- problem is heavy/light pairs are not immideately machine readable as connected pairs, but not all standards contain isotope lables -->
		<content ID="ID215" cvRef="PSI-QC-CV" accession="linked lists" value='4'>{[pep light, pep heavy, gln light, gln heavy],[1,1,1,0]}</content>
	</qualityMetric>
    </runQuality>
    
    <setQuality ID="ID199">
