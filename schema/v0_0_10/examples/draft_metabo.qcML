	<!-- carry over: successive runs (1st standard, 2nd blank) -->
	<qualityMetric ID="ID276" cvRef="PSI-QC-CV" accession="QC:???????" name="carryover index">
		<content ID="ID176" cvRef="PSI-QC-CV" accession="linked lists" value='4'>{[pep light, atp light],'UO:0000187':[0.1,0.05]}</content>
	</qualityMetric>
	<!-- Calibration with external standard -->
	<qualityMetric ID="ID277" cvRef="PSI-QC-CV" accession="QC:???????" name="calibration standard measurement">
		<content ID="ID177" cvRef="PSI-QC-CV" accession="linked lists" value='4'>{[calibration substance1, calbration substance2],' MS:1000042':[6.4E+6,1.4E+8]}</content> <!-- these should correspond to entries in the corresponding calibration standard definition -->
	</qualityMetric>
	<qualityMetric ID="ID278" cvRef="PSI-MS" accession="QC:???????" name="calibration fitx" value="0.98"/>
	<qualityMetric ID="ID279" cvRef="PSI-MS" accession="QC:???????" name="calibration slope" value="1.02"/>
	<!-- RSD for RT (which peaks? and also peak response as intensity?) - Evaluation of precision requires a sufficient number of replicate measurements ... -->
    </setQuality>
    
